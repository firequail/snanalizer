<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/tx
	http://www.springframework.org/schema/tx/spring-tx-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<import resource="hibernateContext.xml" />

	<bean id="usuariosRepository"
		class="snanalizer.data.UsuariosRepositoryImpl" />

	<bean id="redesRepository"
		class="snanalizer.data.RedesRepositoryImpl" />

	<bean id="puntosDeVistaRepository"
		class="snanalizer.data.PuntosDeVistaRepositoryImpl" />

	<bean id="relacionesRepository"
		class="snanalizer.data.RelacionesRepositoryImpl" />

	<bean id="recursosRepository"
		class="snanalizer.data.RecursosRepositoryImpl" />

	<bean id="nodosRepository"
		class="snanalizer.data.NodosRepositoryImpl" />

	<bean id="datosMaestrosRepository"
		class="snanalizer.data.DatosMaestrosRepositoryImpl" />

	<bean id="atributosRepository"
		class="snanalizer.data.AtributosRepositoryImpl" />
		
	<bean id="gruposRecursosRepository"
		class="snanalizer.data.GruposRecursosRepositoryImpl" />
		


</beans>